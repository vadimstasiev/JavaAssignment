<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-min-height: 200; -fx-min-width: 250;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eva.controller.ErrorAlertBox">
   <children>
      <Label fx:id="errorLabel" text="" textFill="#e80000">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Separator prefWidth="200.0" />
      <ToggleButton fx:id="closeButton" onAction="#switchToLogin"  contentDisplay="CENTER" mnemonicParsing="false" prefHeight="60.0" prefWidth="360.0" text="OK" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="24.0" />
         </font>
      </ToggleButton>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
