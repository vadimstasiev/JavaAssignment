<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="221.0" prefWidth="466.0" spacing="20.0" style="-fx-min-height: 200; -fx-min-width: 250;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eva.controller.AlertBox">
   <children>
      <Label fx:id="errorLabel" minHeight="60.0" text="Success message here" textAlignment="CENTER" textFill="#009405" wrapText="true">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Separator prefWidth="200.0" />
      <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ToggleButton fx:id="closeButton" contentDisplay="CENTER" maxWidth="150.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="50.0" prefWidth="360.0" text="OK" textAlignment="CENTER">
               <font>
                  <Font name="Bauhaus 93" size="24.0" />
               </font>
            </ToggleButton>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
