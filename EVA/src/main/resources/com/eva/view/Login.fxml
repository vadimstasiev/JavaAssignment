<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-min-height: 400; -fx-min-width: 250;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eva.controller.Login">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="166.0" prefWidth="360.0" text="EVA" textAlignment="JUSTIFY">
            <font>
                <Font name="Bauhaus 93" size="96.0" />
            </font>
        </Label>
        <TextField editable="false" layoutX="107.0" layoutY="14.0" opacity="0.0">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor></TextField>
      <Separator prefWidth="200.0" />
        <TextField alignment="CENTER" layoutY="140.0" prefHeight="36.0" prefWidth="360.0" promptText="User ID">
            <font>
                <Font size="20.0" />
            </font></TextField>
        <PasswordField alignment="CENTER" layoutX="1.0" layoutY="184.0" prefHeight="36.0" prefWidth="360.0" promptText="Password">
            <font>
                <Font size="20.0" />
            </font></PasswordField>
      <Separator prefWidth="200.0" />
        <ToggleButton contentDisplay="CENTER" layoutY="307.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="360.0" text="login" textAlignment="CENTER" onAction="#switchToRegister">
            <font>
                <Font name="Bauhaus 93" size="24.0" />
            </font>
        </ToggleButton>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
